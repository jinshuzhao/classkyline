server {
    listen 80;
    server_name classskyline;
    charset utf-8;
    client_max_body_size 4M;
    uwsgi_read_timeout 300;

    location = /favicon.ico {
        access_log off;
        log_not_found off;
    }
    location /static/ {
        root /opt;
    }
    location / {
        include uwsgi_params;
        uwsgi_pass unix:/var/run/classskyline.sock;
    }
}
